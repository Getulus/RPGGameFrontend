\n    });\n  }, [])\n\n  const sortEquippment = (equippment) => {\n    for (let item of equippment) {\n      switch (item.slot) {\n        case \"Hands\":\n          setLeftHand(item.image);\n          break;\n          case \"chest\":\n            setChest(item.image);\n            break;\n          case \"foot\":\n            setFoot(item.image);\n            break;\n        default:\n          break;\n      }\n    }\n  };\n\n  const equipItem = (id, itemSlot) => {\n    if (id > 0) {\n      axios\n        .post(`http://localhost:8762/charondor/items/equip`, {\n          id: id,\n          slot: itemSlot,\n        })\n        .then(() => {\n          axios\n            .get(\"http://localhost:8762/charondor/items/equipped-items\")\n            .then((res) => {\n              sortEquippment(res.data);\n            });\n          setUpdateInventory(updateInventory + 1);\n          inventory.map((item) => renderInventory(item));\n          \n        });\n    }\n  };\n\n  const renderInventory = (item) => {\n    let itemId = item.itemID;\n    let itemSlot = item.slot;\n    console.log(itemId);\n    return (\n      <div className=\"inventory-slot\">\n        <img\n          alt=\"\"\n          src={item.image}\n          onClick={() => equipItem(itemId, itemSlot)}\n        ></img>\n      </div>\n    );\n  };\n\n  return (\n    <div className=\"equipment-main-container\">\n      <p className=\"title\">Equipment</p>\n      <p className=\"title\">Inventory</p>\n      {renderEquipment()}\n      <div className=\"inventory-container\">\n        {inventory.map((item) => renderInventory(item))}\n      </div>\n    </div>\n  );\n};\n\nexport default Equipment;\n","/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Character/Equipment/InventoryContext.jsx",[],"/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Character/Equipment/EquippmentContext.jsx",["164"],"import React, { useEffect, useS