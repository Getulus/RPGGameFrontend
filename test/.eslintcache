ter]);\n\n  useEffect(() => {\n    axios\n      .get(\"http://localhost:8762/charondor/items/get-loot\")\n      .then((res) => {\n        setLoot(res.data);\n      })\n      .then(() => {\n        axios.post(\"http://localhost:8762/charondor/items/clear-loot\");\n      });\n  }, [currentPlayer]);\n\n  const renderCombatLogs = (logData) => {\n    return (\n      <div className=\"log-blokk\">\n        <p className=\"stat\">Round: {logData.numberOfRound}</p>\n        <p className=\"stat\">Attacker: {logData.name}</p>\n        <p className=\"stat\">Damage: {logData.damageDealt}</p>\n        <p className=\"stat\">\n          Defender Remaning Health: {logData.enemyRemainingHealth}\n        </p>\n      </div>\n    );\n  };\n\n  const renderLootedItems = (item) => {\n    return (\n      <div className=\"loot-item tooltip\">\n        <div\n          className=\"tooltiptext\"\n          style={{ top: \"105%\", left: \"50%\", marginLeft: \"-80px\" }}\n        >\n          {Object.keys(item).map((stat) => sortItemStat(stat, item))}\n        </div>\n        <img src={item.image}></img>\n      </div>\n    );\n  };\n\n  const checkIfHasBasicLoot = () => {\n    if (combatLog.length > 0) {\n      if (combatLog[combatLog.length - 1][\"name\"] == \"Monster\") {\n        setBasicLoot(<p>No loot</p>);\n      } else {\n        setBasicLoot(\n          <div className=\"basic-loot\">\n            <p>Loot: </p>\n            <div>Gold: {currentMonster.lootedGold}</div>\n            <div>Experience: {currentMonster.experience}</div>\n          </div>\n        );\n      }\n    }\n  };\n\n  const sortItemStat = (stat, item) => {\n    let currentStat = item[stat];\n    if (\n      currentStat != 0 &&\n      stat != \"image\" &&\n      stat != \"itemID\" &&\n      stat != \"dropChance\" &&\n      stat != \"rarity\" &&\n      stat != \"equipped\"\n    ) {\n      if (stat != \"name\" && stat != \"slot\") {\n        return (\n          <div>\n            {stat}: {currentStat}\n          </div>\n        );\n      }\n      return <div style={{ marginBottom: \"5px\" }}>{currentStat}</div>;\n    }\n  };\n\n  return (\n    <div>\n      <div className=\"loot\">\n        {basicLoot}\n        <div className=\"loot-container\">\n          {loot.map((item) => renderLootedItems(item))}\n        </div>\n      </div>\n\n      <p>Combat Log: </p>\n      {combatLog.map((logData) => renderCombatLogs(logData))}\n    </div>\n  );\n};\n\nexport default CombatLog;\n","/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Combat/CombatLogContext.jsx",[],"/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Character/Equipment/Equipment.jsx",["161","162","163","164","165","166","167","168","169","170","171","172","173","174","175","176","177","178","179","180","181","182","183"],"import \"./EquipmentStyle.css\";\nimport React, {\n  BrowserRouter as Router,\n  useState,\n  useContext,\n  useEffect,\n} from \"react\";\nimport { InventoryContext } from \"./InventoryContext\";\nimport { Link } from \"react-router-dom\";\nimport axios from \"axios\";\nimport { EquippmentContext } from \"./EquippmentContext\";\nimport { CharacterContext, CharactersContext } from \"../CharacterContext\";\n\nconst Equipment = () => {\n  const [\n    inventory,\n    setInventory,\n    updateInventory,\n    setUpdateInventory,\n  ] = useContext(InventoryContext);\n\n  const [update, setUpdate] = useContext(CharactersContext);\n\n  const [\n    leftHand,\n    setLeftHand,\n    rightHand,\n    setRigthHand,\n    head,\n    setHead,\n    chest,\n    setChest,\n    hands,\n    setHands,\n    neck,\n    setNeck,\n    foot,\n    setFoot,\n  ] = useContext(EquippmentContext);\n\n  const renderEquipment = () => {\n    return (\n      <div className=\"equipment-container\">\n        <div id=\"left-hand\" className=\"slot tooltip\">\n          <div className=\"tooltiptext\">\n            {Object.keys(leftHand).map((stat) => sortItemStat(stat, leftHand))}\n          </div>\n          <img src={leftHand.image} alt=\"\"></img>\n        </div>\n\n        <div id=\"head\" className=\"slot tooltip\">\n          <img src={head.image}></img>\n          <div className=\"tooltiptext\">\n            {Object.keys(head).map((stat) => sortItemStat(stat, head))}\n          </div>\n        </div>\n\n        <div id=\"right-hand\" className=\"slot tooltip\">\n          <img src={rightHand.image}></img>\n          <div className=\"tooltiptext\">\n            {Object.keys(rightHand).map((stat) =>\n              sortItemStat(stat, rightHand)\n            )}\n          </div>\n        </div>\n\n        <div id=\"neck\" className=\"slot tooltip\">\n          <img src={neck.image}></img>\n          <div className=\"tooltiptext\">\n            {Object.keys(neck).map((stat) => sortItemStat(stat, neck))}\n          </div>\n        </div>\n\n        <div id=\"hands\" className=\"slot tooltip\">\n          <img src={hands.image}></img>\n          <div className=\"tooltiptext\">\n            {Object.keys(hands).map((stat) => sortItemStat(stat, hands))}\n          </div>\n        </div>\n\n        <div id=\"chest\" className=\"slot tooltip\">\n          <img src={chest.image}></img>\n          <div className=\"tooltiptext\">\n            {Object.keys(chest).map((stat) => sortItemStat(stat, chest))}\n          </div>\n        </div>\n\n        <div id=\"foot\" className=\"slot tooltip\">\n          <img src={foot.image}></img>\n          <div className=\"tooltiptext\">\n            {Object.keys(foot).map((stat) => sortItemStat(stat, foot))}\n          </div>\n        </div>\n      </div>\n    );\n  };\n\n  useEffect(() => {\n    axios\n      .get(\"http://localhost:8762/charondor/items/equipped-items\")\n      .then((res) => {\n        sortEquippment(res.data);\n      });\n  }, []);\n\n  const sortEquippment = (equippment) => {\n    for (let item of equippment) {\n      switch (item.slot) {\n        case \"leftHand\":\n          setLeftHand(item);\n          break;\n        case \"chest\":\n          setChest(item);\n          break;\n        case \"foot\":\n          setFoot(item);\n          break;\n        case \"rightHand\":\n          setRigthHand(item);\n          break;\n        case \"neck\":\n          setNeck(item);\n          break;\n        case \"head\":\n          setHead(item);\n          break;\n        case \"gloves\":\n          setHands(item);\n          break;\n        default:\n          break;\n      }\n    }\n  };\n\n  const equipItem = (id, itemSlot) => {\n    if (id > 0) {\n      axios\n        .post(`http://localhost:8762/charondor/items/equip`, {\n          id: id,\n          slot: itemSlot,\n        })\n        .then(() => {\n          axios\n            .get(\"http://localhost:8762/charondor/items/equipped-items\")\n            .then((res) => {\n              sortEquippment(res.data);\n            });\n          setUpdateInventory(updateInventory + 1);\n          inventory.map((item) => renderInventory(item));\n        });\n    }\n  };\n\n  const renderInventory = (item) => {\n    let itemId = item.itemID;\n    let itemSlot = item.slot;\n    console.log(item);\n    if (item.name == \"empty slot\") {\n      return (\n        <div className=\"inventory-slot\">\n          <img alt=\"\" src={item.image}></img>\n        </div>\n      );\n    } else {\n      return (\n        <div className=\"inventory-slot tooltip\">\n          <div className=\"tooltiptext\">\n            {Object.keys(item).map((stat) => sortItemStat(stat, item))}\n          </div>\n          <img\n            alt=\"\"\n            src={item.image}\n            onClick={() => equipItem(itemId, itemSlot)}\n          ></img>\n        </div>\n      );\n    }\n  };\n\n  const sortItemStat = (stat, item) => {\n    let currentStat = item[stat];\n    if (\n      currentStat != 0 &&\n      stat != \"image\" &&\n      stat != \"itemID\" &&\n      stat != \"dropChance\" &&\n      stat != \"rarity\" &&\n      stat != \"equipped\"\n    ) {\n      if (stat != \"name\" && stat != \"slot\") {\n        return (\n          <div>\n            {stat}: {currentStat}\n          </div>\n        );\n      }\n      return <div style={{ marginBottom: \"5px\" }}>{currentStat}</div>;\n    }\n  };\n\n  return (\n    <div className=\"equipment-main-container\">\n      <p className=\"title\">Equipment</p>\n      <p className=\"title\">Inventory</p>\n      {renderEquipment()}\n      <div className=\"inventory-container\">\n        {inventory.map((item) => renderInventory(item))}\n      </div>\n    </div>\n  );\n};\n\nexport default Equipment;\n","/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Character/Equipment/InventoryContext.jsx",[],"/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Character/Equipment/EquippmentContext.jsx",["184"],"import React, { useEffect, useState, createContext } from \"react\";\n\nexport const EquippmentContext = createContext();\n\nexport const EquippmentCollection = (props) => {\n  const [leftHand, setLeftHand] = useState({image: \"/images/left-hand.png\"});\n  const [rightHand, setRigthHand] = useState({image : \"/images/right-hand.png\"});\n  const [head, setHead] = useState({image : \"/images/head.png\"});\n  const [chest, setChest] = useState({image : \"/images/chest.png\"});\n  const [hands, setHands] = useState({image : \"images/hand.png\"});\n  const [neck, setNeck] = useState({image : \"images/legs.png\"});\n  const [foot, setFoot] = useState({image : \"images/foot.png\"});\n  return (\n    <EquippmentContext.Provider\n      value={[leftHand, setLeftHand,  rightHand, setRigthHand, head, setHead, chest, setChest, hands, setHands, neck, setNeck, foot, setFoot]}\n    >\n      {props.children}\n    </EquippmentContext.Provider>\n  );\n};","/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Creation/ChooseClass.jsx",