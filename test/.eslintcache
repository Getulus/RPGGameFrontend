div id=\"battle-log\" className=\"combat-panel\">\n          <CombatLog></CombatLog>\n        </div>\n      </div>\n      <button className=\"button\" onClick={() => startCombat()}>\n        Start Combat\n      </button>\n      <button className=\"button\" onClick={() => regenerate()}>\n        End Combat\n      </button>\n    </div>\n  );\n};\n\nexport default CombatPage;\n","/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Combat/CombatPlayerStats.jsx",["123","124","125","126","127","128","129","130","131"],"import React, { useState, useContext, useEffect } from \"react\";\nimport {\n  CharacterCollection,\n  CharactersContext,\n} from \"../Character/CharacterContext\";\nimport axios from \"axios\";\n\n\nconst CombatPlayerStats = () => {\n  const [\n    currentPlayer,\n    setCurrentPlayer,\n    currentMonster,\n    setCurrentMonster,\n    update,\n    setUpdate,\n  ] = useContext(CharactersContext);\n\n  return (\n    <div className=\"combat-player-stats\">\n    <p>Player Info: </p>\n      <p className=\"stat\">Level: {currentPlayer.level}</p>\n      <p className=\"stat\">Current Health: {currentPlayer.currentHealth}</p>\n      <p className=\"stat\">Attack Value: {currentPlayer.attackValue}</p>\n      <p className=\"stat\">Defense Value: {currentPlayer.defenseValue}</p>\n      <p className=\"stat\">Critical Chance: {currentPlayer.criticalChance}%</p>\n    </div>\n  );\n};\n\nexport default CombatPlayerStats;\n","/home/getulus/my_project/Jobhunt/1st project frontend/RPGGameFrontend/test/src/Components/Pages/Combat/CombatMonsterStats.jsx",["132","133","134","135","136","137","138","139"],"import React, { useState, useContext, useEffect } from \"react\";\nimport {\n  CharacterCollection,\n  CharactersContext,\n} from \"../Character/CharacterContext\";\n\n\nconst CombatPlayerStats = () => {\n  const [\n    currentPlayer,\n    setCurrentPlayer,\n    currentMonster,\n    setCurrentMonster,\n    update,\n    setUpdate,\n  ]